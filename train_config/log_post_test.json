{
    "name": "post_test_log_gboosting",
    "description": "Add new module called post test to check the outcome after train. Measure the metrics by actual distance. Would have bigger penalty on greater value",
    "repository": {
        "experiment_id": "experiment_001",
        "data": {
            "type": "sqlite",
            "properties": { 
                "name": "example.db",
                "migrate": true
            }
        },
        "object": {
            "type": "sqlite",
            "properties": { 
                "name": "example.db",
                "migrate": true
            }
        }
    },
    "instructions": [
        {
            "type": "data_io",
            "properties": {
                "path": "dataset",
                "file": "cfs_2017",
                "format": "csv",
                "reference": "commodity_flow"
            },
            "call": [
                {
                    "type": "load",
                    "n_rows": 10000
                }
            ]
        },
        {
            "type": "data_cleaner",
            "properties": { "type": "lazy_call",  "reference": "commodity_flow"},
                
            "call": [
                {"type": "filter_columns", "columns": ["shipment_weight", "shipment_value", "naics"]},
                {"type": "drop_na"}
            ]
        },
        {
            "type": "data_transformer",
            "properties": { 
                "type": "lazy_call", 
                "reference": "commodity_flow" 
            },
            "call": [
                {
                    "type": "log_transformation",
                    "condition": "replace",
                    "columns": ["shipment_weight", "shipment_value"]
                },
                {
                    "type": "one_hot_encoding",
                    "condition": "append_and_remove",
                    "columns": ["naics"]
                }
            ]
        },
        {
            "type": "model_trainer",
            "properties": { 
                "objective": "best_model",
                "random_state": 42,
                "fold": 5,
                "parameter_grid": {
                    "type": "exhaustive"
                },
                "primary_metric": "mae",
                "metrics": ["mae"]
            },
            "call": [
                {
                    "model_type": "gradient_boosting_regressor",
                    "hyperparameters": {
                        "n_estimators": [200],
                        "learning_rate": [0.1],
                        "max_depth": [5]
                    }
                }
            ]
        },
        {
            "type": "post_test",
            "properties": {
                "path": "dataset",
                "file": "cfs_2017",
                "format": "csv",
                "reference": "commodity_flow"
            },
            "call": [
                {
                    "intent": "measure_outcome_actual_value",
                    "check_against": "random",
                    "n_samples": 100000,
                    "metrics": ["mae"],
                    "seed": 42
                }
            ]
        }
    ]
}